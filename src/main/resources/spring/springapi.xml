<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="company" class="com.zl.springapi.service.pojo.Company">
        <property name="aliveTime" value="12"/>
        <property name="location" value="南山大道"/>
        <property name="name" value="腾讯"/>
    </bean>
    <bean id="hello" class="com.zl.springapi.service.pojo.Hello" init-method="initMethod" destroy-method="destroyMethod">
        <constructor-arg name="resultStr" value="today is Sunday"/>
    </bean>
</beans>